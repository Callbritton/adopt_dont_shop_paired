<%= form_tag "/reviews/#{params[:review_id]}", method: :post do %>
    <%= label_tag :title %>
    <br>
    <%= text_field_tag :title %>
    <br>
    <%= label_tag :rating %>
    <br>
    <%= text_field_tag :rating %>
    <br>
    <%= label_tag :content %>
    <br>
    <%= text_field_tag :content %>
    <br>
    <%= label_tag :optional_picture %>
    <br>
    <%= text_field_tag :optional_picture %>
    <br>
    <%= submit_tag 'Edit Review' %>
  <% end %>
